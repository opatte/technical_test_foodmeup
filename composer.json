{
    "name": "opatte/technical_test_foodmeup",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.2.*",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/polyfill-apcu": "^1.0",

        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",

        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/migrations": "1.5.0",
        "doctrine/doctrine-migrations-bundle": "1.2.1",
        "doctrine/doctrine-fixtures-bundle": "2.3.0",

        "incenteev/composer-parameter-handler": "^2.0",

        "jms/serializer-bundle": "^1.0",
        "willdurand/hateoas": "2.10.0",
        "friendsofsymfony/rest-bundle": "2.1.1",

        "knplabs/knp-paginator-bundle": "~2.4",
        "nelmio/alice": "2.2.2",
        "cocur/slugify": "^1.2",
        "league/csv": "~7.0",

        "ramsey/uuid": "^3.5",
        "chrisguitarguy/request-id-bundle": "^2.0",

        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",

        "behat/behat": "~3.0",
        "behat/gherkin": "~4.3",
        "behat/symfony2-extension": "~2.0",

        "behat/mink": "~1.6",
        "behat/mink-extension": "~2.0",
        "behat/mink-browserkit-driver": "~1.2",
        "behat/mink-goutte-driver": "~1.1",
        "behat/mink-selenium2-driver": "1.2.0",
        "behatch/contexts": "dev-master",
        "emuse/behat-html-formatter": "0.1.*",
        "adamquaile/behat-command-runner-extension": "^0.1.1",

        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.1.*",
        "sebastian/phpcpd": "2.0.*",
        "pdepend/pdepend": "2.0.*",
        "phploc/phploc": "2.0.*",
        "phpunit/phpunit": "5.6.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
